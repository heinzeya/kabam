{
  "name": "mwc_meta",
  "version": "0.0.2",
  "description": "Meta repository to do unit and integration tests on all parts of mwc_core application.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "./node_modules/.bin/mocha --reporter spec",
    "blanket": { "pattern": ["index.js", "models/**/*.js", "public/**/*.js", "routes/**/*.js"] }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mywebclass/mwc_meta.git"
  },
  "keywords": [
    "mwc",
    "mwc_core",
    "meta"
  ],
  "author": "Anatolij Ostroumov",
  "license": "MIT",
  "dependencies": {
    "mwc_core": "git+https://github.com/mywebclass/mwc_core.git",
    "mwc_plugin_example": "git+https://github.com/mywebclass/mwc_plugin_example.git",
    "mwc_plugin_spine": "git+https://github.com/mywebclass/mwc_plugin_spine.git"
  },
  "bugs": {
    "url": "https://github.com/mywebclass/mwc_meta/issues"
  },
  "devDependencies": {
    "grunt": "~0.4.1",
    "grunt-cli": "*",
    "grunt-contrib-watch": "~0.5.0",
    "grunt-contrib-jshint": "~0.6.0",
    "should": "~1.2.2",
    "mocha": "~1.12.0",
    "grunt-mocha-cov": "0.0.7",
    "blanket": "~1.1.5"
  }
}
